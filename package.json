    "@actions/github": "^6.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@jest/globals": "^30.2.0",
    "@octokit/types": "^14.0.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.15.29",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }

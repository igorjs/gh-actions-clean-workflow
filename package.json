{
  "name": "@igorjs/gh-actions-clean-workflow",
  "version": "7.0.0",
  "description": "Github Action to delete old workflow runs",
  "engines": {
    "node": ">=24.0.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src",
    "build": "ncc build src/index.ts -o dist --minify --no-source-map-register --target es2022 --license licenses.txt",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "all": "npm run lint && npm run test && npm run build"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "JavaScript"
  ],
  "author": "Igor J. S. <oss@mail.igorjs.io> (https://igorjs.io/)",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@octokit/types": "^16.0.0",
    "@types/node": "^24.9.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vercel/ncc": "^0.38.4",
    "@vitest/coverage-v8": "^4.0.6",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.6"
  }
}
